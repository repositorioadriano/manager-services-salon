<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] e-GESTÃO</title>

    <environment names="Development">
        <link href="http://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.8/css/materialize.min.css">
        <link href="~/css/site.css" type="text/css" rel="stylesheet" media="screen,projection">
    </environment>
    <environment names="Staging,Production">
        <link href="http://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.8/css/materialize.min.css">
        <link href="~/css/site.css" type="text/css" rel="stylesheet" media="screen,projection">
    </environment>
</head>
<body>
    <div class="fixed-action-btn toolbar">
        <a class="btn-floating btn-large orange">
        <i class="large material-icons">add</i>
        </a>
        <ul>
        <li class="waves-effect waves-light"><a href="#!"><i class="material-icons">note_add</i></a></li>
        <li class="waves-effect waves-light"><a asp-action="Create"><i class="material-icons">contacts</i></a></li>
        <li class="waves-effect waves-light"><a href="#!"><i class="material-icons">supervisor_account</i></a></li>
        <li class="waves-effect waves-light"><a href="#!"><i class="material-icons">shopping_basket</i></a></li>
        <li class="waves-effect waves-light"><a href="#!"><i class="material-icons">work</i></a></li>
        </ul>
    </div>
    <nav>
        <ul id="slide-out" class="side-nav fixed">
            <li><a href="#!"><i class="material-icons">dashboard</i>Dashboard</a></li>
            <li><a href="#!"><i class="material-icons">note_add</i>Agendas</a></li>
            <li><a asp-controller="Clientes" asp-action="Index"><i class="material-icons">contacts</i>Clientes</a></li>
            <li><a href="#!"><i class="material-icons">assignment_ind</i>Funcionários</a></li>
            <li><a href="#!"><i class="material-icons">work</i>Serviços</a></li>
            <li><div class="divider"></div></li>
            <li><a class="subheader">CADASTROS</a></li>
            <li><a href="#!"><i class="material-icons">note_add</i>Nova Agenda</a></li>
            <li><a asp-controlller="Clientes" asp-action="Create"><i class="material-icons">contacts</i>Novo Cliente</a></li>
            <li><a href="#!"><i class="material-icons">assignment_ind</i>Novo Funcionário</a></li>
            <li><a href="#!"><i class="material-icons">work</i>Novo Serviço</a></li>
            
    </ul>
    <a href="#" data-activates="slide-out" class="button-collapse"><i class="material-icons">menu</i></a>
        <div class="nav-wrapper teal">
        <a href="#!" class="brand-logo">Manager Services</a>
        
       
        </div>
    </nav>
    <main> 
    <!-- Page Layout here -->
    <br>
    <div class="row">
        
      <div class="col s12 m4 l2"> 
        
      </div>
      
      <div class="col s12 m8 l9">
          @RenderBody()
      </div>
    </div>
    </main>
    <footer class="page-footer teal">
                <div class="container">
                    <div class="row">
                    <div class="col l6 s12">
                        <h5 class="white-text">Manager Services</h5>
                    </div>
                    </div>
                </div>
                <div class="footer-copyright">
                    <div class="container">
                    © Manager Services 2016. Todos os direitos reservados
                    </div>
                </div>
        </footer>

    <environment names="Development">
        <script src="~/lib/jquery/dist/jquery.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>
    </environment>
    <environment names="Staging,Production">
        <script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-2.2.0.min.js"
                asp-fallback-src="~/lib/jquery/dist/jquery.min.js"
                asp-fallback-test="window.jQuery">
        </script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.8/js/materialize.min.js"
                asp-fallback-src="~/lib/bootstrap/dist/js/bootstrap.min.js"
                asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal">
        </script>
        <script src="~/js/site.js" asp-append-version="true"></script>
        <script src="~/js/JQuery.InputMask.js" type="text/javascript"></script>
    </environment>

    @RenderSection("scripts", required: false)
</body>
</html>